<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xmz.service.hr.mapper.BaseCitysMapper">

    <select id="queryallByProvUuid" parameterType="String" resultType="com.xmz.service.hr.entity.pojo.BaseCitys">
        select * from base_citys where PROV_UUID=#{id}
    </select>
</mapper>
